/**
 * Definition for binary tree
 * class TreeNode {
 *     int val;
 *     TreeNode left;
 *     TreeNode right;
 *     TreeNode(int x) {
 *      val = x;
 *      left=null;
 *      right=null;
 *     }
 * }
 */
public class Solution {
    public int solve(TreeNode A) {
        HashSet<Long> h=new HashSet<Long>();
        long sum=getsum(A,h);
        if(sum%2==0 && h.contains(sum/2))
         return 1;
        return 0;
    }
    public long getsum(TreeNode node,HashSet<Long> h)
    {
        if(node==null)
        {
            return 0;
        }
        else{
        long sum=node.val+getsum(node.left,h)+getsum(node.right,h);
        h.add(sum);
        return sum;
        }
        
    }
}
