public class Solution {
    public int solve(String A, String B) {
    int n=A.length();
    int m=B.length();
    int i,j;
    int arr[][]=new int[n+1][m+1];
    for(i=n;i>=0;i--)
    {
        for(j=m;j>=0;j--)
        {
            if(i>=n || j>=m)
             arr[i][j]=0;
            else if(A.charAt(i)==B.charAt(j))
              arr[i][j]=1+arr[i+1][j+1];
            else
              arr[i][j]=Math.max(arr[i+1][j],arr[i][j+1]);
        }
    }
    return arr[0][0];
}
}
