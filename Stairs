public class Solution {
    public int climbStairs(int A) {
        int ans[]=new int[A+1];
        ans[0]=0;
        ans[1]=1;
      if(A==1)
         return 1;
        if(A==2)
         return 2;
        else
         {
         ans[2]=2;
         for(int i=3;i<ans.length;i++)
        {
            ans[i]=ans[i-1]+ans[i-2];
        }
    }
     return ans[A];
    }
}
