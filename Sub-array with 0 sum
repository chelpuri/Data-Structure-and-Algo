public class Solution {
    public int solve(ArrayList<Integer> A) {
        HashMap<Long,Long> hmap = new HashMap<>();
        long sum=0;
        hmap.put(sum,new Long(1));
        for(int i=0;i<A.size();i++)
        {
            sum+=A.get(i);
            
            if(hmap.get(sum)==null)
             hmap.put(sum,new Long(1));
            else
             return 1;
        }
        
        return 0;
    }
}
