public class Solution {
    public int solve(int A) {
        
        int x=1;
        int y=2;
        if(A==1)
          return x;
        if(A==2)
          return y;
        for(int i=3;i<=A;i++)
        {
            int z=y+(i-1)*x;
            x=y%10003;
            y=z%10003;
        }
      return y%10003;


    }
}
